    "version": "0.2.0",
    "configurations": [
        {
            "name": "VIRUS - Downloader",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/DataDownloader.py",
            "console": "integratedTerminal",
            "args": ["--date_range", "XX/01/2021", "YY/01/2021"]
        },
        {
            "name": "VIRUS - Date Range",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/DataDownloader.py",
            "console": "integratedTerminal",
            "args": ["--get_date_range"]
        },
        {
            "name": "VIRUS - ETL",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/ETL.py",
            "console": "integratedTerminal",
            "args": [ ]
        },
        {
            "name": "VIRUS - Vaccini Performed Download",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/VacciniDownload.py",
            "console": "integratedTerminal",
            "args": ["--download_vaccinazioni"]
        },
        {
            "name": "VIRUS - Vaccini Delivered Download",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/VacciniDownload.py",
            "console": "integratedTerminal",
            "args": ["--download_consegne"]
        },
        {
            "name": "VIRUS - Vaccini Chart",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/VacciniDownload.py",
            "console": "integratedTerminal",
            "args": ["--chart"]
        }
    ]
}   